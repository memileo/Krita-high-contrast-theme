QSlider::groove:horizontal {
    border: 0px;
	border-radius: 2px;
    height: 4px;
    background: QPalette.ToolTipBase(l: 1.85);
    margin: 2px 2px;;
}

QSlider::handle:horizontal {
    background: QPalette.Button(l: 2.09);
    border: 1px solid QPalette.ButtonText(l: 0.76);
    width: 14px;
    margin: -6px 0;
    border-radius: 8px;
}

QSlider::handle:horizontal:hover {
    background: QPalette.Button(l: 2.09);
    border: 1px solid QPalette.ButtonText(l: 1.09);
    width: 14px;
    margin: -6px 0;
    border-radius: 8px;
}

QSlider::handle:horizontal:pressed {
    background: QPalette.Highlight(l: 0.97);
    border: 1px solid QPalette.ButtonText(l: 1.09);
    width: 14px;
    margin: -6px 0;
    border-radius: 8px;
}

QSlider::add-page:horizontal {
    background: QPalette.ToolTipBase(l: 1.85);
	height: 4px;
	border: 0px;
	border-radius: 2px;
	margin: 2px 2px;;
}

QSlider::sub-page:horizontal {
    background: QPalette.ButtonText(l: 0.89);
	height: 4px;
	border: 0px;
	border-radius: 2px;
	margin: 2px 2px;;
}

