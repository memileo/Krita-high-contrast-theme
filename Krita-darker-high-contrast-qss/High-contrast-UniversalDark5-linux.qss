/* -----------------------------------
	ComboBox
   -----------------------------------
*/

QComboBox {
	background-color: QPalette.Button(l: 1.80);
	selection-background-color: QPalette.Highlight;
    border: 1px solid QPalette.ButtonText(l: 0.76);
    border-radius: 6px;
    padding-top: 0px;
	padding-bottom: 0px;
	padding-left: 8px;
	padding-right: 8px;
    min-width: 50px;
	min-height: 21px;
	max-height: 21px;
	margin-left: 4px;
	margin-right: 4px;
	margin-bottom: 4px;
	margin-top: 3px;
	color: QPalette.ButtonText;
}

QComboBox:disabled {
	border: 1px solid QPalette.ButtonText(l: 0.66);
	color: QPalette.ButtonText(l: 0.66);
	background-color: QPalette.ToolTipBase(l: 0.94);
}

KisWidgetChooser QComboBox {
	margin-left: 3px;
	margin-right: 3px;
	max-width: 50px;
}

QComboBox::hover {
	background-color: QPalette.Button(l: 1.80);
    border-color: QPalette.ButtonText(l: 0.89);
	color: QPalette.ButtonText(l: 1.09);
}


QComboBox::drop-down {
    width: 19px;
	margin: 1px;
	padding: -1px;
    border: 1px solid QPalette.Highlight(l: 1.50, s: 2.4);
    border-radius: 5px;
	background-color: QPalette.Highlight;
	/*background: qlineargradient(x0:0, y0:1, x1:0, y1:1,
                stop:0 QPalette.Highlight, stop:1 QPalette.Highlight(l: 1.16));*/ /* Gradient test*/
}

QComboBox::drop-down:disabled {
	background-color: QPalette.ButtonText(l: 0.66);
}

QComboBox::down-arrow {
    color: QPalette.ButtonText(l: 1.09);
    image: url(stylesheet:images/arrows.svg).QPalette.Highlight(l:2 s: 2.1);
	/*image: url(stylesheet:images/arrows.svg).QPalette.Highlight(h:160, l:1.3, s: 1.4, a: 0.9);*/
}

QComboBox QAbstractItemView {
	border: 1px solid QPalette.ButtonText(l: 0.78);
	color: QPalette.ButtonText(l: 1.09);
	background-color: QPalette.Base(l: 0.86);
	selection-color: QPalette.HighlightedText;
	selection-background-color: QPalette.Highlight;
	padding: 4px;
	/*border-radius: 6px;*/  /* korners */
}

