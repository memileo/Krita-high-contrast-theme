/* -----------------------------------
	Scrollbars
   -----------------------------------
*/

.QScrollBar:horizontal {
	max-height: 16px;
	padding-top: 3px;
	padding-bottom: 2px;
	padding-left: 3px;
	padding-right: 3px;
	margin: 0;
	background: transparent;
}
.QScrollBar:vertical {
	max-width: 16px;
	padding: 3px 2px 3px 3px;
	padding-top: 3px;
	padding-bottom: 3px;
	padding-left: 3px;
	padding-right: 2px;
	background: transparent;
}
.QScrollBar::handle:vertical, .QScrollBar::handle:horizontal {
	background: QPalette.ButtonText(0.84); /* force.QPalette.ButtonText */
	border-style: solid;
	border-radius: 4px;
	border-width: 1px;
	border-color: QPalette.ButtonText(0.89); /* force.QPalette.ButtonText */
	min-height: 25px;
	min-width: 25px;
}
.QScrollBar::handle:horizontal:hover, .QScrollBar::handle:vertical:hover {
	background-color: QPalette.ButtonText(0.98); /* force.QPalette.ButtonText */
	border-style: solid;
	border-width: 1px;
	border-color: QPalette.ButtonText(1.09); /* force.QPalette.ButtonText */
}
.QScrollBar::add-line, .QScrollBar::sub-line {
    border: 0px;
    border-radius: 0px;
}

