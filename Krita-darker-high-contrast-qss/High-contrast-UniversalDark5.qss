QMdiSubWindow::title {
	background-color: QPalette.ToolTipBase(0.86);
	selection-background-color: orange;
	border-bottom: 3px solid QPalette.ButtonText(0.84);
	border-radius: 0px;
	height: 12px;
	padding: 6px;
	margin: -3px;
	color: QPalette.Highlight(0.00);
	image: url();
}

QMdiSubWindow:maximized, QMdiSubWindow::title:maximized {
	border: 0px solid QPalette.Window;
	background-color: QPalette.Window;
/*	padding-bottom: 4px;*/
}

QMdiSubWindow::close-button, QMdiSubWindow::minimize-button, QMdiSubWindow::maximize-button, QMdiSubWindow::normal-button {
   /* background: QPalette.Highlight(1.45);*/
	background: QPalette.ToolTipBase(0.86);
	border: 1px solid QPalette.ToolTipBase(0.86);
	width: 11px;
	height: 11px;
	padding: 2px;
    border-radius: 8px;
/*	margin-left: 4px;
	margin-right: 4px;*/
	subcontrol-position: left;
}

QMdiSubWindow::close-button:maximized, QMdiSubWindow::minimize-button:maximized, QMdiSubWindow::maximize-button:maximized, QMdiSubWindow::normal-button:maximized {
	background: QPalette.Window;
	border: 1px solid QPalette.Window;
/*	width: 8px;
	height: 8px;
	border-radius: 7px;*/
	subcontrol-position: top;
}

QMdiSubWindow::close-button:hover, QMdiSubWindow::minimize-button:hover, QMdiSubWindow::maximize-button:hover, QMdiSubWindow::normal-button:hover {
/*    border-color: QPalette.ButtonText(0.89);*/
	background: QPalette.Window(1.64);
}

QMdiSubWindow::close-button:pressed, QMdiSubWindow::minimize-button:pressed, QMdiSubWindow::maximize-button:pressed, QMdiSubWindow::normal-button:pressed {
    background: QPalette.ButtonText(1.09); /* force.QPalette.ButtonText */
}

/* -----------------------------------
	Tabs
   -----------------------------------
*/

QTabBar {
	qproperty-drawBase: 0;
}

QTabWidget::pane {
    border: 1px solid QPalette.ToolTipBase(1.85);
	border-radius: 4px;
    position: absolute;
    top: -16px;
	bottom: 4px;
	padding-top: 16px;
	background-color: QPalette.Button(1.04);
}

QTabWidget::tab-bar {
    alignment: center;
}

QTabBar::tab {
	border-style: solid;
	border-color: transparent;
	color: QPalette.WindowText(1.09); /* force.QPalette.WindowText */
	background-color: QPalette.Window(1.68); /* force.QPalette.Window */
	font-size: 11px;
	padding-left: 4px;
	padding-right: 4px;
	padding-top: 2px;
	padding-bottom: 3px;
	margin-bottom: 4px;
}

QTabBar::tab:first {
	border-top-left-radius: 6px;
	border-bottom-left-radius: 6px;
	margin-right: -4px;
	margin-left: 4px;
	padding-right: 8px;
	padding-left: 8px;
}

QTabBar::tab:last {
	border-top-right-radius: 6px;
	border-bottom-right-radius: 6px;
	margin-left: -4px;
	margin-right: 4px;
	padding-left: 8px;
	padding-right: 8px;
}

QTabBar::tab:middle {
	margin-left: -4px;
	margin-right: -4px;
	padding-left: 8px;
	padding-right: 8px;
}

QTabBar::tab:selected, QTabBar::tab:last:selected {
	border-color: QPalette.ButtonText(0.96); /* force.QPalette.ButtonText */
	border-width: 1px;
	border-style: solid;
	border-radius: 6px;
	color: QPalette.HighlightedText(0.00);
	background-color: QPalette.Highlight(1.40); /* force.QPalette.Highlight */
}

QTabBar::tab:hover, QTabBar::tab:last:hover {
	color: QPalette.ButtonText(0.96); /* force.QPalette.ButtonText */
}

QTabBar::tab:selected:hover {
	color: QPalette.Button(0.89);
}

QTabBar::tab:pressed, QTabBar::tab:last:pressed {
	color: QPalette.HighlightedText(0.00);
	font-size: 10px;
	padding-bottom: 2px;
}

